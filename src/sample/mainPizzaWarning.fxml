<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sample.Pizza"
            prefHeight="100" prefWidth="350"
            stylesheets="@styles.css">

    <content>
        <GridPane>
            <Label GridPane.rowIndex="0"></Label>
            <Label GridPane.rowIndex="1"></Label>
            <Label fx:id="warningLabel" text="Incorrect price, order cancelled!" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
        </GridPane>

    </content>


</DialogPane>
